<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/ActivityMathematicsNormal"
    tools:context=".system.ActivityMathematicsNormal">

    <ImageView
        android:id="@+id/imageFoneMathematics"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:foregroundGravity="center"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fone_normal_act" />

    <TextView
        android:id="@+id/imageBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:foregroundGravity="left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/Back"
        android:textSize="60sp"
        android:fontFamily="@font/bowlby_one_sc"
        android:textColor="@color/normal"
        android:textStyle="bold"/>
    <ImageButton
        android:id="@+id/imageRestartNormalMath"
        android:layout_width="80sp"
        android:layout_height="85sp"
        android:layout_margin="10sp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/style_buttons_diagram_restart"
        android:gravity="center"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageDiagramNormalMath"
        app:srcCompat="@android:drawable/ic_menu_rotate"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageDiagramNormalMath"
        android:layout_width="80sp"
        android:layout_height="85sp"
        android:layout_marginRight="5sp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/style_buttons_diagram_restart"
        android:gravity="center"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/diagram"
        tools:ignore="SpeakableTextPresentCheck" />

