<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/nameButtonAct2_2" />
            <stroke android:width="6sp"></stroke>
            <stroke android:color="@color/nameButtonAct2"></stroke>
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid
                android:color="@color/nameButtonAct2" />
            <stroke android:width="4sp"></stroke>
            <stroke android:color="@color/nameButtonAct2"></stroke>


            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
</selector>